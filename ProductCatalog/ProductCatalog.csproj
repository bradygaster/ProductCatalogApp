<Project Sdk="Microsoft.NET.Sdk.Web">

  <PropertyGroup>
    <TargetFramework>net10.0</TargetFramework>
    <Nullable>disable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <RootNamespace>ProductCatalog</RootNamespace>
    <AssemblyName>ProductCatalog</AssemblyName>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
  </PropertyGroup>

  <ItemGroup>
    <Compile Remove="App_Start\**" />
    <Compile Remove="Global.asax.cs" />
    <Compile Remove="Properties\**" />
    <Content Remove="App_Start\**" />
    <Content Remove="Global.asax" />
    <Content Remove="Properties\**" />
    <EmbeddedResource Remove="App_Start\**" />
    <EmbeddedResource Remove="Properties\**" />
    <None Remove="App_Start\**" />
    <None Remove="Properties\**" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="13.0.3" />
    <PackageReference Include="System.ServiceModel.Http" Version="8.0.0" />
    <PackageReference Include="System.ServiceModel.Primitives" Version="8.0.0" />
  </ItemGroup>

</Project>
